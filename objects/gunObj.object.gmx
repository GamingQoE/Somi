    curent_mouse_x = mouse_x;
    curent_mouse_y = mouse_y;
}
else
{
    currentMouseState = is_mouse_down[0];
    curent_mouse_x = pre_mouse_x[0];
    curent_mouse_y = pre_mouse_y[0];
    for(i=0;i&lt;(delay_base_frame-1);i++)
    {
        is_mouse_down[i] = is_mouse_down[i+1]
        pre_mouse_x[i] = pre_mouse_x[i+1];
        pre_mouse_y[i] = pre_mouse_y[i+1];
    }
    if(mouse_check_button(mb_left))
        is_mouse_down[delay_base_frame-1]=1;
    else
        is_mouse_down[delay_base_frame-1]=0;
        
    pre_mouse_x[delay_base_frame-1]=mouse_x;
    pre_mouse_y[delay_base_frame-1]=mouse_y;
        
}


if(mouse_x &gt; x and somiObj.sprite_index!=somiDamageStarSpr)
{
    a=arctan((curent_mouse_y - y)/(curent_mouse_x - x));
